snippet p
	print(${1})${2}
snippet fun
	function ${1:funname}(${2})
		${3}
	end${4}
snippet f
	function(${1}) ${2} end${3}
snippet s
	sep()
snippet repeat
	repeat
		${1}
		i = i+1
	until i > 5
snippet for
	for ${1:i} = ${2:1}, ${3:10}${4:, 1} do
	  ${5}
	end
snippet e
	os.exit(0)
	${1}
snippet r
	return ${1}
snippet req
	require("${1}")${2} 
snippet env
	os.getenv("${1:HOME}")${2}
snippet tmp
	os.tmpname()
